#include<bits/stdc++.h>
using namespace std;

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

#define ll long long

//====================================

void solve(){
    ll n; cin>>n;
    string dum = "";
    char c='A';
    while(n--){
        dum = dum+c+dum;
        ++c;
    }
    cout<<dum;
}


int main(){

#ifndef ONLINE_JUDGE
    freopen("in.txt", "r", stdin);
    freopen("out.txt", "w", stdout);
#endif

    optimize();

    ll t=1;
    // cin>>t;
    while(t--)
        solve();
}
